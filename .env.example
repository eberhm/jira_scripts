# Copy to .env and fill values (do NOT commit .env)

# Jira Configuration
JIRA_BASE_URL=https://your-org.atlassian.net
JIRA_EMAIL=you@example.com
JIRA_API_TOKEN=your_api_token

# Google Gemini Configuration
GOOGLE_API_KEY=your_gemini_api_token

# GitHub Configuration (for search-replace script)
GITHUB_TOKEN=your_github_personal_access_token

# Optional GitHub Search-Replace Configuration
# GITHUB_ORGANIZATIONS=new-work,xing-com
# SEARCH_STRING=project-metadata.xing.io
# REPLACEMENT_STRING=project-metadata.nwse.io
# DRY_RUN=true

# Logging
LOG_LEVEL=INFO
